{
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "name": "Chromium OS Iteration Viewer",
  "version": "3.0",
  "description": "Shows the current Chromium OS Iteration when looking at the issue tracker.",
  "icons": {
    "128": "icon-128x128.png"
  },
  "page_action": {
    "default_title": "Current ChromiumOS Iteration"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation"
  ]
}
